[
    //--------------------------------
    // custom
    //--------------------------------

    // docker container attach
    {
        "key": "ctrl+d ctrl+a",
        "command": "vscode-docker.containers.attachShell"
    },

    // 次のタブに移動
    {
      "key": "ctrl+tab",
      "command": "workbench.action.nextEditor"
    },
    {
      "key": "shift+cmd+]",
      "command": "-workbench.action.nextEditor"
    },
    // 前のタブに移動
    {
      "key": "ctrl+shift+tab",
      "command": "workbench.action.previousEditor"
    },
    {
      "key": "alt+cmd+left",
      "command": "-workbench.action.previousEditor"
    }
]
